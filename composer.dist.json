{
  "name": "politsturm",
  "type": "project",
  "description": "Politsturm in docker",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "version": "1.0.0",
        "dist": {"url": "https://ru.wordpress.org/wordpress-4.9.8-ru_RU.zip", "type": "zip"}
      }
    },
    {
      "type": "package",
      "package": {
        "name": "politsturm/wordpress-theme",
        "version": "1.0.0",
        "source": {"url": "https://github.com/politsturm/website.git", "type": "git", "reference": "fix_tag_skipping"},
        "dist": {
          "url": "https://github.com/politsturm/website/archive/master.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "composer",
      "url": "file:///var/www/runtime/packages.json"
    },
    {
      "packagist.org": false
    }
  ],
  "require": {
    "wordpress/wordpress": "@stable",
    "politsturm/wordpress-theme": "@stable",
    "politsturm/composer-wordpress-plugin": "@stable",
    "wordpress-plugin/admin-menu-editor": "1.8.5",
    "wordpress-plugin/advanced-custom-fields": "5.7.7",
    "wordpress-plugin/all-in-one-wp-security-and-firewall": "4.3.7.2",
    "wordpress-plugin/all-in-one-seo-pack": "2.9.1",
    "wordpress-plugin/autoptimize": "2.4.2",
    "wordpress-plugin/classic-editor": "0.5",
    "wordpress-plugin/contact-form-7": "5.0.5",
    "wordpress-plugin/custom-post-type-ui": "1.6.0",
    "wordpress-plugin/html-on-pages": "1.1",
    "wordpress-plugin/limit-login-attempts": "1.7.1",
    "wordpress-plugin/onesignal-free-web-push-notifications": "1.16.9",
    "wordpress-plugin/p3-profiler": "1.5.3.9",
    "wordpress-plugin/regenerate-thumbnails": "3.0.2",
    "wordpress-plugin/rustolat": "0.3",
    "wordpress-plugin/scripts-to-footerphp": "0.6.3",
    "wordpress-plugin/social-networks-auto-poster-facebook-twitter-g": "4.2.8",
    "wordpress-plugin/wp-smushit": "2.9.1"
  },
  "require-dev": {}
}